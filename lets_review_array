#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include<string.h>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int i,n,a,b,k=0,l=0,j,m,h2,h3;
    char s[30];
    char s1[30],temp[30],temp1[30],temp2[30],temp3[30];
    scanf("%d",&n);
    scanf("%s",s);
    scanf("%s",s1);
    a=strlen(s);
    b=strlen(s1);
    for(i=0;i<a;i++)
        {
        if(i%2==0)
            {
            
            temp[k]=s[i];
            k++;
        }
        else{
          
            temp1[l]=s[i];
              l++;
            }
    
    }
    int h=strlen(temp);
    int h1=strlen(temp1);
    for(i=0;i<h;i++)
        printf("%c",temp[i]);
    for(j=0;i<n;j++);
        printf(" ");
    for(m=0;m<h1;m++)
        printf("%c",temp1[m]);
    
