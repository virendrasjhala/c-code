#include<stdio.h>
#include<stdlib.h>
int f(int x , int *py ,int **ppz )
{
    int y , z;
    **ppz =**ppz + 1;
    z = **ppz;
    *py = *py +2;
    y = *py;
    x = x + 3;

    return x + y + z;
}
int main()
{
    int c , *b , **a;
    c = 4;
    b = &c;
    a = &b;
    printf(" %d",f(c,b,a));
    return 0;

}
